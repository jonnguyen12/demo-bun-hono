{
	"name": "demo-bun-hono",
	"scripts": {
		"dev": "bun run --hot src/index.ts",
		"seed": "bun prisma/seed.ts",
		"build": "bun prisma generate && bun build ./src/index.ts --outdir ./dist --target bun",
		"start": "bun ./dist/index.js"
	},
	"dependencies": {
		"@prisma/client": "^6.5.0",
		"hono": "^4.7.4"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"prisma": "^6.5.0"
	},
	"packageManager": "bun@1.2.5",
	"engines": {
		"bun": ">=1.2.5",
		"node": ">=22"
	}
}
